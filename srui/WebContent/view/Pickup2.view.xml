<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	controllerName="sap.it.sr.ui.view.Pickup2" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page 
		title="{i18n>pickupTitle}"
		id="pickupPage">

		<footer>
			<Bar>
				<contentRight>
					<Button class="sapUiSmallMarginEnd" id="pickup" text="{i18n>pkCaption}"
						type="Emphasized" visible="true" press="handlePickupPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>

</core:View>